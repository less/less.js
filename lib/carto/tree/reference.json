                "default-value": "none"
            },
            "meta-output": {
                "css": "text-meta-output",
                "type": "string",
                "default-value": "",
                "default-meaning": "No MetaWriter Output"
            },
            "meta-writer": {
                "css": "text-meta-writer",
                "type": "string",
                "default-value": ""
            }
        },
